{
  "name": "menu-api",
  "version": "1.0.0",
  "description": "API for a simple menu app",
  "main": "src/main.ts",
  "scripts": {
    "build": "rimraf dist && tsc --project tsconfig.build.json && tsc-alias",
    "test": "jest --detectOpenHandles",
    "prestart": "npm run build",
    "start": "node dist/main.js",
    "dev": "nodemon --watch \"src/\" --exec \"ts-node -r tsconfig-paths/register src/main.ts\" -e ts",
    "lint": "eslint \"src/**\"",
    "lint:fix": "eslint --fix \"src/**\"",
    "format": "prettier --check \"src/**\"",
    "format:fix": "prettier --write \"src/**\""
  },
  "keywords": [
    "boilerplate",
    "node",
    "express",
    "typescript"
  ],
  "repository": "https://github.com/nielsero/menu-api",
  "author": "nielsero",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.4",
    "@types/pino": "^7.0.5",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^3.15.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "pino": "^8.17.1",
    "pino-pretty": "^10.3.0",
    "zod": "^3.22.4"
  }
}
